<div class="container">
    <div class="row">
        <div class="span3">
            <h4>Some Orders</h4>
            <ul class="unstyled">
                {{#each model}}
                    {{#linkTo 'orders' this}}<li>
                        {{name}} <small class="text-success">{{camera}}</small> <small class="muted"> {{date created}}</small>
                    </li>{{/linkTo}}
                {{/each}}
            </ul>
        </div>
        <div class="span8">
            {{outlet}}
        </div>
    </div>
</div>